{
    "name": "padosoft/invisible-recaptcha",
    "description": "Invisible reCAPTCHA For Laravel.",
    "keywords": [
        "recaptcha",
        "no-captcha",
        "captcha",
        "invisible",
        "invisible-recaptcha",
        "laravel",
        "laravel5",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Albert Chen",
            "homepage": "https://www.albert-chen.com"
        },
        {
            "name": "Leonardo Padovani",
            "homepage": "https://www.padosoft.com"
        }
    ],
    "require": {
        "php": "^5.6.4 || ^7.0 || ^8.0 || ^8.1 || ^8.2",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/view": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Padosoft\\InvisibleReCaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Padosoft\\InvisibleReCaptcha\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Padosoft\\InvisibleReCaptcha\\InvisibleReCaptchaServiceProvider"
            ]
        }
    }
}
